{
  "name": "cms-hitech-apd-api",
  "version": "1.0.0",
  "description": "APD api for CMS HITECH program",
  "main": "main.js",
  "scripts": {
    "audit": "npx audit-ci --config ./audit-ci.json",
    "lint": "eslint '**/*.js'",
    "make-migrate": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "plop": "plop",
    "seed": "knex seed:run",
    "start": "node main.js",
    "start-dev": "nodemon ./main.js -e js",
    "jest": "NODE_ENV=test jest",
    "tap": "NODE_ENV=test tap -J --no-browser --no-coverage --no-timeout ${TESTS:-'{,!(node_modules)/**/}*.test.js'}",
    "test": "NODE_ENV=test tap -J --cov --coverage-report=lcov --no-browser --no-timeout --reporter=spec --test-arg=--silent '{,!(node_modules)/**/}*.test.js'",
    "test-endpoints": "jest --detectOpenHandles --runInBand '.+\\.endpoint\\.js'",
    "test-specific": "NODE_ENV=test tap --cov --coverage-report=lcov --no-browser --reporter=spec",
    "prettier": "prettier --single-quote --trailing-comma none --write \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/eAPD.git"
  },
  "keywords": [
    "cms",
    "medicaid",
    "hitech",
    "emr",
    "apd"
  ],
  "contributors": [
    "Nicolas Aretakis",
    "Heather Battaglia",
    "Ronald Bronson",
    "Bill Brooks",
    "Richard Davis",
    "Jenn Downs",
    "Nicole Fenton",
    "Tiffany Forkner",
    "Eghosa Guobadia",
    "Austin Hernandez",
    "James Hupp",
    "Jesse James",
    "Meghana Khandekar",
    "Jerome Lee",
    "Nikki Lee",
    "Christopher (CM) Lubinksi",
    "Aviva Oskow",
    "Paul Phillips",
    "Laura Ponc√©",
    "Brendan Sudol",
    "Caryn Swartz",
    "Jen Thibault",
    "Greg Walker",
    "Nichole Weems"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/CMSgov/eAPD/issues"
  },
  "homepage": "https://github.com/CMSgov/eAPD#readme",
  "dependencies": {
    "ajv": "^6.12.0",
    "aws-sdk": "^2.646.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.3",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "jsonpatch": "^3.0.1",
    "jsonpointer": "^4.0.1",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.20.13",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "uuid": "^3.4.0",
    "winston": "^2.4.4",
    "zxcvbn": "^4.4.2"
  },
  "engines": {
    "node": "^10.14"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.2.1",
    "nodemon": "^2.0.4",
    "plop": "^2.7.4",
    "prettier": "^1.19.1",
    "sinon": "^8.1.1",
    "stream-mock": "^2.0.5",
    "tap": "^14.10.8"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test|endpoint).js?(x)"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
