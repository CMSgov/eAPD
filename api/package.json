  "homepage": "https://github.com/CMSgov/eAPD#readme",
  "dependencies": {
    "@okta/jwt-verifier": "^2.1.0",
    "@okta/okta-sdk-nodejs": "^4.5.1",
    "ajv": "^8.1.0",
    "aws-sdk": "^2.888.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fast-json-patch": "^3.0.0-1",
    "form-data": "^4.0.0",
    "jsonpatch": "^3.0.1",
    "jsonpatch-to-mongodb": "^1.0.0",
    "jsonpointer": "^4.1.0",
    "knex": "^0.95.4",
    "migrate-mongo": "^8.2.2",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.6",
    "mongoose-patch-history": "^2.0.0",
    "mongoose-patcher": "^0.2.15",
